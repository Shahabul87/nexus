{
  "PROJECT_NAME": "MedAssist CHW",
  "VERSION": "1.0.0",

  "PATHS": {
    "RAW_DATA_DIR": "./data/raw",
    "CLEAN_DATA_DIR": "./data/processed",
    "PROTOCOLS_DIR": "./data/protocols",
    "TEST_CASES_DIR": "./data/test_cases",
    "FINETUNING_DIR": "./data/finetuning",

    "MODEL_DIR": "./models/converted",
    "MODEL_CACHE_DIR": "./models/cache",
    "CHECKPOINT_DIR": "./models/checkpoints",

    "SUBMISSION_DIR": "./submission",
    "OUTPUT_DIR": "./output",

    "MOBILE_APP_DIR": "./src/mobile",
    "BACKEND_DIR": "./src/backend"
  },

  "MODELS": {
    "MEDGEMMA_4B": {
      "huggingface_id": "google/medgemma-4b-it",
      "local_path": "./models/converted/medgemma_4b_int4.tflite",
      "quantization": "INT4",
      "target_size_mb": 2000
    },
    "MEDSIGLIB": {
      "huggingface_id": "google/medsiglib",
      "local_path": "./models/converted/medsiglib_int8.tflite",
      "quantization": "INT8",
      "target_size_mb": 400
    },
    "HEAR": {
      "huggingface_id": "google/hear",
      "local_path": "./models/converted/hear_int8.tflite",
      "quantization": "INT8",
      "target_size_mb": 100
    },
    "DERM_FOUNDATION": {
      "huggingface_id": "google/derm-foundation",
      "local_path": "./models/converted/derm_int8.tflite",
      "quantization": "INT8",
      "target_size_mb": 150
    },
    "CXR_FOUNDATION": {
      "huggingface_id": "google/cxr-foundation",
      "local_path": "./models/converted/cxr_int8.tflite",
      "quantization": "INT8",
      "target_size_mb": 200
    }
  },

  "MOBILE_CONFIG": {
    "min_android_sdk": 24,
    "min_ios_version": "14.0",
    "target_devices": ["mid-range Android phones", "iPhone 11+"],
    "max_model_memory_mb": 3000,
    "inference_timeout_ms": 5000
  },

  "COMPETITION": {
    "name": "MedGemma Impact Challenge",
    "url": "https://www.kaggle.com/competitions/med-gemma-impact-challenge",
    "deadline": "2026-02-24T23:59:00Z",
    "tracks": ["Main Track", "Edge AI Prize"]
  }
}
